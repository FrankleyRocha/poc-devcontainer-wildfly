{
	"name": "AMBIENTE-DEV-WILDFLY",
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/home/ubuntu/workspace",
	"shutdownAction": "stopCompose",
	"customizations": {
		"vscode": {
			"extensions": [
				"pleiades.java-extension-pack-jdk"
			]
		}
	},
	"forwardPorts": [
		8080
	],
	"portsAttributes": {
		"8080": {
			"label": "app",
			"onAutoForward": "notify"
		}
	},
	"remoteUser": "ubuntu",
	"containerUser": "ubuntu"

}